<div class="col-md-12 title">{{geneID}}</div>

<div class="col-md-12 gene_name greyish">({{gene_name}})</div>

<div class="col-md-12 process">{{process}}</div>

<div class="col-md-12 function greyish">{{gene_function}}</div>

{{#if chr}}
<div class="row infos">
<div class="col-md-5 miniTitle greyish">
    chr
</div>                
<div class="col-md-7 info">{{chr}}</div>
</div>
{{/if}}

{{#if geneID_a1}}
<div class="row infos">
<div class="col-md-5 miniTitle greyish">
    {{geneID_a1_name}}
</div>                
<div class="col-md-7 info">{{geneID_a1}}</div>
{{/if}}
</div>

{{#if geneID_a2}}
<div class="row infos">
<div class="col-md-5 miniTitle greyish">
    {{geneID_a2_name}}
</div>                
<div class="col-md-7 info">{{geneID_a2}}</div>
{{/if}}
</div>

{{#if geneID_a3}}
<div class="row infos">

<div class="col-md-5 miniTitle greyish">
    {{geneID_a3_name}}
</div>                
<div class="col-md-7 info">{{geneID_a3}}</div>
</div>
{{/if}}

<div class="col-md-12 miniTitle" style="font-weight: 700;">
    Up-regulated sample:
</div>
<div class="col-md-9 miniTitle">
&emsp;No. of sample
</div>
                
<div class="col-md-3">{{upfreq}}</div>

<div class="col-md-9 miniTitle">
&emsp;Mean Log2 Fold Change
</div>
                
<div class="col-md-3">{{uplog2}}</div>

<div class="col-md-12 miniTitle" style="font-weight: 700;">
    Down-regulated sample:
</div>
<div class="col-md-9 miniTitle">
&emsp;No. of sample
</div>
                
<div class="col-md-3">{{downfreq}}</div>

<div class="col-md-9 miniTitle">
&emsp;Mean Log2 Fold Change 
</div>
                
<div class="col-md-3">{{downlog2}}</div>
<hr>
<div class="col-md-9 miniTitle" style="font-weight: 700;">
    No. of mutation data
</div>
                
<div class="col-md-3">{{mutation}}</div>